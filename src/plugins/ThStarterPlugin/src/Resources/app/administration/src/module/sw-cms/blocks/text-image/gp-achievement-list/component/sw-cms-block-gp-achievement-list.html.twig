{% block sw_cms_block_gp_achievement_list %}
	<div class="sw-cms-block-gp-achievement-list">
		<div class="sw-cms-block-gp-achievement-list">
			<div class="sw-cms-block-gp-achievement-list__top">
				<slot name="first-text"></slot>
				<slot name="second-text"></slot>
			</div>

			<div class="sw-cms-block-gp-achievement-list__center">
				<slot name="third-text"></slot>
				<slot name="fourth-text"></slot>
			</div>

			<div class="sw-cms-block-gp-achievement-list__bottom">
				<slot name="fifth-text"></slot>
				<slot name="sixth-text"></slot>
			</div>

			<div class="sw-cms-block-gp-achievement-list__main">
				<slot name="long-text"></slot>
				<slot name="main-image"></slot>
			</div>
		</div>
	</div>
{% endblock %}
